 RowBox[{
  RowBox[{"SequenceTree", "[", "n_", "]"}], ":=", 
  RowBox[{"NestTree", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"TrueQ", "[", 
        RowBox[{"#", "==", "a"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"a", ",", "b", ",", "bInv"}], "}"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"TrueQ", "[", 
          RowBox[{"#", "==", "b"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "b", ",", "aInv"}], "}"}], ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"TrueQ", "[", 
            RowBox[{"#", "==", "aInv"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"b", ",", "aInv", ",", "bInv"}], "}"}], ",", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"TrueQ", "[", 
              RowBox[{"#", "==", "bInv"}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"a", ",", "aInv", ",", "bInv"}], "}"}], ",", 
             RowBox[{"{", "}"}]}], "]"}]}], "]"}]}], "]"}]}], "]"}], "&"}], 
    ",", 
    RowBox[{"Tree", "[", 
     RowBox[{"{", 
      RowBox[{"a", ",", "b", ",", "aInv", ",", "bInv"}], "}"}], "]"}], ",", 
    "n"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"SequenceTree", "[", "1", "]"}]}], "Input",
